source 'https://rubygems.org' do
  # pinning activesupport, newer versions require zeitwerk
  # zeitwerk is currently not available on our build hosts
  gem 'activesupport', '~> 5.2', '>= 5.2.4.3'
  # and rubyzip too. >= 2.0.0 needs ruby 2.4
  gem 'rubyzip', '1.3.0'
  gem 'github-pages', '204'
  gem 'html-proofer', '3.15.3'
end
